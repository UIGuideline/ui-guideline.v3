{
  "buildCommand": "pnpm build",
  "outputDirectory": "apps/web/dist",
  "installCommand": "pnpm install",
  "devCommand": "pnpm dev",
  "framework": null,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "redirects": [
    {
      "source": "/",
      "destination": "/docs/components",
      "permanent": true
    },
    {
      "source": "/docs",
      "destination": "/docs/components",
      "permanent": true
    }
  ]
}
